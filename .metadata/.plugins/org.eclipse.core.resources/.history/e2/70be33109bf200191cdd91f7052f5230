package com.syntax.class4;

public class nestedloopexi {
	public static void main(String[] args) {
		
		//print 
		/*
		 * 123456789
		 * 123456789
		 * 123456789
		 * 123456789
		 * 123456789
		 */
			
				 for( int r=1; r<5; r++) {// r for rows 
				 
				 for (int c=0;c<10; c++) {// c for cols 
					 System.out.print(c);
				 }
				 System.out.println();
		
				 }	
				 System.out.println("--------------------");
				 
				 //print 
				 /*
				  * 54321
				  * 54321
				  * 54321
				  * 54321
				  * 54321
				  */
				 
				 for( int r=5; r>=0; r--) {// r for rows 
					 
					 for (int c=5;c>=1; c--) {// c for cols 
						 System.out.print(c);
					 }
					 System.out.println();
				 }
				 System.out.println("--------------------------------");
				 
				//print 
				// *
				 //**
				 //***
				 //****
				 //*****
				 
				 for (int i=1; i<=5; i++) {
					 
				 for  (int j=1; j<=i; j++) {
					 System.out.print("*");
				 }
				 System.out.println();
				 
				 
				 
				 
				 
				 }		 
	}
}



