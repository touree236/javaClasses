package com.syntax.class4;

public class nestedLoopTask {
	public static void main(String[] args) {
		
		//print the following patterns 
		
		//55555
		//4444
		//333
		//22
		//1
		
		 for (int i=5; i>=1; i--) {
			 
			 for  (int j=5; j>=i; j--) {
				 System.out.print(j);
			 }
			 System.out.println();
	}
System.out.println("----------------------------");

// print the following pattern 
//*
//**
//***
//****
//*****
//****
//***
//**
//*

for (int i=9; i>=1; i--) {
	 
	 for  (int j=9; j<=i; j++) {
		 System.out.print(j);
	 }
	 System.out.println();

}
}
}