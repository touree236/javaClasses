package com.syntax.class2review;

public class ForLoopPractice {
	public static void main(String[] args) {
		
		// print even number form 1- 100  
		
		for( int i=1; i<=100; i++) {
			System.out.print(i+" ");
		}
		
		
		
		
			System.out.println("--------------------------------");
			
			
			
			
			
		}
	
	}
