				System.out.println(str.replaceAll("[a-zA-Z]", ""));//Leave only numbers 1423445645465456
				
				//Remove everything except text and numbers
				String str2="Hi#$%How#$%#$4356346";
				System.out.println(str2.replaceAll("[^a-zA-Z0-9]", ""));
				
				
				
				
