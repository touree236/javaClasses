package com.class13;

public class StringManipulationSubstringNov2 {
public static void main(String[] args) {
	
	//  this method returns a new string that is a substring of this string 
         
	
	
String str="Java is getting intersting";

System.out.println(str.substring(6));



System.out.println(str.substring(8, 15));  // most useful method (sub string)

	
	
	
}
}
