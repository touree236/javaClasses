package com.syntax.class2review;

public class nestedReview {
	/*I want to eat if
	 *  i have money
	 * if the restaurant is near by 
	 * I will eat at the restaurant 
	 * 
	 * otherwise, I will not eat outside
	 */
	
	public static void main(String[] args) {
		
	boolean hasMoney=true;
	boolean isRestNear=true;
	
	if (hasMoney==true);
	if (isRestNear) {
	System.out.println("I will eat in the restruant");	
	} else {
		System.out.println("Cant walk long distance"); // we use else if , if we have multiple condition
	}
}
}