package com.syntax.class2review;

public class ForLoopPractice {
	public static void main(String[] args) {
		
		// print even number form 20 to 1 ( 2 way) 
		
		for( int i=20; i>=0; i--) {
			if (i%2 i=0) {
			System.out.println(i);
		}
		
		}
		// print odd numbers between 20 and 50 (2 ways) 
		
	

		for ( int i=20; i<=50; i++) {
			System.out.println(i);
			
		}
	}

}